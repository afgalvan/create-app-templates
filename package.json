  "devDependencies": {
    "@cucumber/cucumber": "^7.3.1",
    "@types/cors": "^2.8.12",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.0",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
